#!/bin/bash
# make-bundle

DIR=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
UNITY_PATH="/Applications/Unity 4.6/Unity.app/Contents/MacOS/Unity"

echo "" > bundle-log.txt

"$UNITY_PATH" -batchmode -projectPath ~/Workspace/AR-rigging -quit -executeMethod MakeBundle.BundleCommandLine -logFile "$DIR/bundle-log.txt" -3dify:bundle-files Models/paul_20000_blender/model_20000.obj -3dify:bundle-output $(pwd)/model.bin

#cat $DIR/bundle-log.txt
